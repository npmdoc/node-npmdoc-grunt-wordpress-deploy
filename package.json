{
    "author": "kai zhu <kaizhu256@gmail.com>",
    "buildNpmdoc": "grunt-wordpress-deploy",
    "description": "#### basic api documentation for  [grunt-wordpress-deploy (v0.0.6)](https://github.com/webrain/grunt-wordpress-deploy)  [![npm package](https://img.shields.io/npm/v/npmdoc-grunt-wordpress-deploy.svg?style=flat-square)](https://www.npmjs.org/package/npmdoc-grunt-wordpress-deploy) [![travis-ci.org build-status](https://api.travis-ci.org/npmdoc/node-npmdoc-grunt-wordpress-deploy.svg)](https://travis-ci.org/npmdoc/node-npmdoc-grunt-wordpress-deploy)",
    "devDependencies": {
        "electron-lite": "kaizhu256/node-electron-lite#alpha",
        "grunt-wordpress-deploy": "*",
        "utility2": "kaizhu256/node-utility2#alpha"
    },
    "engines": {
        "node": ">=4.0"
    },
    "homepage": "https://github.com/npmdoc/node-npmdoc-grunt-wordpress-deploy",
    "keywords": [
        "documentation",
        "grunt-wordpress-deploy"
    ],
    "license": "MIT",
    "main": "lib.npmdoc_grunt_wordpress_deploy.js",
    "name": "npmdoc-grunt-wordpress-deploy",
    "nameAlias": "npmdoc_grunt_wordpress_deploy",
    "nameOriginal": "npmdoc-grunt-wordpress-deploy",
    "os": [
        "darwin",
        "linux"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/npmdoc/node-npmdoc-grunt-wordpress-deploy"
    },
    "scripts": {
        "build-ci": "utility2 shReadmeTest build_ci.sh",
        "env": "env",
        "heroku-postbuild": "(set -e; npm install \"kaizhu256/node-utility2#alpha\"; utility2 shDeployHeroku)",
        "postinstall": "if [ -f npm_scripts.sh ]; then ./npm_scripts.sh postinstall; fi",
        "start": "(set -e; export PORT=${PORT:-8080}; utility2 start test.js)",
        "test": "(set -e; export PORT=$(utility2 shServerPortRandom); utility2 test test.js)"
